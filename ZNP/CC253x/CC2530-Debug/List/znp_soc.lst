     62          #define MOD_IF             4     // ~Modulation bit of MDMTEST1 register.
     63          #define TX_PWR_MOD__SET(MOD_) st ( \
     64            if ((MOD_)) \
     65            { \
     66              MDMTEST1 |= BV(MOD_IF); \
     67            } \
     68            else \
     69            { \
     70              MDMTEST1 &= ~BV(MOD_IF); \
     71            } \
     72          );
     73          
     74          #define TX_PWR_TONE_SET(TONE) st ( \
     75            MDMTEST0 &= ~0xF0; \
     76            MDMTEST0 |= (TONE << 4); \
     77          )
